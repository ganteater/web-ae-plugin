<?xml version='1.0' encoding='UTF-8'?>
<Recipe name="Web Page">
	<Runnable name="cookiesFilter" threadLog="cookiesFilter">
		<Var name="enaledCookies" type="array">
			<item>-GAPS</item> 
		</Var> 
		<Extern class="com.ganteater.ae.desktop.processor.Web">
			<Loop>
				<DeleteCookie except="enaledCookies" /> 
				<Wait delay="1000" /> 
			</Loop> 
		</Extern> 
	</Runnable> 
	<Extern class="com.ganteater.ae.desktop.processor.Web">
		<Threads name="cookiesFilter" threadLog="cookiesFilter" mode="nowait" /> 
		<Page url="http://gmail.com" /> 
		<Text id="identifierId" value="viktor.tovstyi@gmail.com" /> 
		<Click xpath="/html/body/div[1]/div[1]/div[2]/div[2]/div/div/div[2]/div/div[2]/div/div[1]/div/span/span" /> 
		<Var name="email_password" type="password" init="console" /> 
		<Text value="$var{email_password}">
			<name>password</name> 
		</Text> 
		<Click xpath="/html/body/div[1]/div[1]/div[2]/div[2]/div/div/div[2]/div/div[2]/div/div[1]/div/span/span" /> 
	</Extern> 
</Recipe> 
